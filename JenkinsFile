pipeline {
        agent none
        stages {

           stage('Build'){
               agent any
                steps{
                    echo 'Building'
                 }
           }
          stage("Sonar Qube Build Analysis") {
            agent any
            steps {
              withSonarQubeEnv('SonarQube') {
                bat 'mvn clean package sonar:sonar'
              }
            }
          }
          
        }
}
    
    
        
